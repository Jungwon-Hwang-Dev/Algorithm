for tc in range(1,1+int(input())):
    n = int(input())
    price = [int(n) for n in input().split()]
    profit = 0

    while(price):
        max_price = max(price)
        idx_max = price.index(max_price)
        profit += max_price*idx_max - sum(price[:idx_max])
        del price[:idx_max+1]

    print(f'#{tc} {profit}')
